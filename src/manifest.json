{
  "name": "g60.jp URL shortener",
  "description": "g60.jpでURLを短縮するよ",
  "version": "0.15",
  "manifest_version": 3,
  "action": {
    "default_icon": "icon128.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "all_frames": false,
      "matches": ["<all_urls>"],
      "exclude_matches": [],
      "js": [
        "jquery-3.6.0.min.js"
      ]
    }
  ]
}
